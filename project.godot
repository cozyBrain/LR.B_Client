run/main_scene="res://terminal.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

window="*res://utils/window.gd"
terminal="*res://terminal.gd"
directory="*res://utils/directory.gd"
node="*res://session/virtual_remote_hub/space/objects/nodes/node.gd"
c="*res://constants.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[dotnet]

